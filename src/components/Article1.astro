---
import Layout from '../layouts/Layout.astro';
import Sharan from '../assets/Sharan.jpg'
import Dattu from '../assets/Dattu.jpg'
import Chandan from '../assets/Chandan.jpg'
import Shirish from '../assets/Shirish.jpg'
import sudesh_naik from '../assets/sudesh_naik.jpg'
import Anand from '../assets/Anand.jpg'
import Manju from '../assets/Manju.jpg'
