---
import Layout from '../layouts/Layout.astro';
---

<article class="talktalk" id="contact">
    <div class="talkcolor">
        <div class="talkhead">
            <h1>Talk To Us</h1>
            <p>Samarth always happy to help you.</p>
        </div>
        <div class="talkflexcol">
            <div class="talkaddress hide">
                <h1>Keep in touch</h1>
                <h2>Samarth Meditech Private Limited</h2>
                <p>Software Technology Parks of India <br/>4th Floor, IT Park , Hubballi - 580 029 <br/>Karnataka – India <br/>Talk to us - 6364731111 / 9606973333 <br/><br/>Thank you.</p>
            </div>
            <form action="https://formsubmit.co/f665dd9b28932b2984a73da1a384b77b" method="POST">
                <div class="talkflex">
                    <div class="talkname talkbox">
                        <input type="text" id="name" name="name" placeholder="Full Name" required>
                    </div>
                    <div class="talkemail talkbox">
                        <input type="text" id="email" name="email" placeholder="E-mail Address" required>
                    </div>
                    <div class="talkcno talkbox">
                        <input type="text" id="cno" name="cno" placeholder="Contact Number" required>
                    </div>
                </div>
                
                <div class="talkmessage">
                        <textarea id="message" name="message" rows="9" cols="46" placeholder="Your Message"></textarea>
                </div>
                <button class="talkbutton">Send Message</button>
            </form>
        </div>
    </div>        
</article>
<footer class="talkend">
    <div class="talkicons">
        <div class="talkicon"><img src="src/assets/icons/facebook.png" alt=""></div>
        <div class="talkicon"><img src="src/assets/icons/linkedin.png" alt=""></div>
        <div class="talkicon"><img src="src/assets/icons/twitter.png" alt=""></div>
    </div>
    <hr class="talkhr">
    <div class="talkcopyright"><p>COPYRIGHT © 2020 SAMARTH MEDITECH PVT LTD COMPANY</p></div>
</footer>
