---
interface Props{
    title:string;
};
const {title}=Astro.props;

import Navbar from '../components/Navbar.astro';
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}/blog</title>
    <style>
        @font-face{
            font-family: "Inter";
            src: url("/fonts/Inter-Regular.woff2") format("woff2");
            font-display: swap;
            font-weight: 400;

        }
        @font-face{
            font-family: "Inter";
            src: url("/fonts/Inter-Italic.woff2") format("woff2");
            font-display: swap;
            font-weight: 400;
            font-style: italic;
            
        }
        @font-face{
            font-family: "Inter";
            src: url("/fonts/Inter-Bold.woff2") format("woff2");
            font-display: swap;
            font-weight: 700;
            
        }
        
</style>
</head>
<body>
    <Navbar/>
    <div class="max-w-5xl mx-auto mt-5 pl-3">
        <slot/>
    </div>
</body>
</html>